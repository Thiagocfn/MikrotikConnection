{
  "name": "thiagocfn/mikrotikconnection",
  "homepage": "https://github.com/thiagocfn/mikrotikconnection",
  "license": "MIT",
  "type": "library",
  "description": "Biblioteca em PHP para conectar e extrair informações de wi-fi de um mikrotik",
  "require": {
    "php": ">=5.4.16",
    "pear2/net_routeros": "1.0.0b6",
    "pear2/net_transmitter": "1.0.0b2"
  },
  "authors": [
    {
      "name": "Thiago Souza",
      "email": "thiagocfn@msn.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "codacy/coverage": "1.4.*"
  },
  "autoload": {
    "psr-4": {
      "Thiagocfn\\MikrotikConnection\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Thiagocfn\\MikrotikConnection\\": "tests/"
    }
  }
}
